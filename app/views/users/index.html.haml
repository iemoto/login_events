%div test
= link_to "ログアウト", destroy_user_session_path, method: :delete, class:'btn'
= link_to "new event", new_user_event_path(current_user.id)

%p#notice= notice
%h1 Events
%table
  %thead
    %tr
      %th Title
      %th Description
      %th Start date
      %th End date
      %th{:colspan => "3"}
  %tbody
    - @events.each do |event|
      %tr
        %td= event.title
        %td= event.description
        %td= event.start_date
        %td= event.end_date
